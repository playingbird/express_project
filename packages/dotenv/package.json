{
  "name": "@tinyhttp/dotenv",
  "version": "0.0.5",
  "description": "0-dependency module to load envinronmental variables from .env to process.env",
  "type": "module",
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json",
    "./": "./"
  },
  "homepage": "https://github.com/talentlessguy/tinyhttp#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/talentlessguy/tinyhttp.git",
    "directory": "packages/dotenv"
  },
  "author": "v1rtl",
  "license": "MIT",
  "keywords": [
    "tinyhttp",
    "node.js",
    "web framework",
    "web",
    "backend",
    "dotenv"
  ],
  "engines": {
    "node": ">=12.4.0"
  },
  "scripts": {
    "build": "tsup src/index.ts --minify-whitespace --format cjs,esm --dts"
  },
  "devDependencies": {
    "tsup": "^3.6.1",
    "typescript": "^4.0.2"
  }
}
