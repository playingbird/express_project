{
  "name": "@tinyhttp/favicon",
  "version": "0.0.3",
  "type": "module",
  "description": "favicon middleware",
  "homepage": "https://github.com/talentlessguy/tinyhttp#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/talentlessguy/tinyhttp.git",
    "directory": "packages/favicon"
  },
