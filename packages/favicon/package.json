  "license": "MIT",
  "devDependencies": {
    "@types/ms": "^0.7.31",
    "tsup": "^3.6.1",
    "typescript": "^4.0.2"
  },
  "scripts": {
    "build": "tsup src/index.ts  --format cjs,esm --dts"
  },
  "dependencies": {
    "@tinyhttp/etag": "workspace:*",
    "es-fresh": "^0.0.8",
    "ms": "^2.1.2"
