{
  "name": "@tinyhttp/cookie-signature",
  "version": "0.1.10",
  "description": "HTTP cookie signing and unsigning",
  "homepage": "https://github.com/talentlessguy/tinyhttp#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/talentlessguy/tinyhttp.git",
    "directory": "packages/cookie-signature"
  },
  "engines": {
    "node": ">=12.4.0"
  },
  "type": "module",
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json",
    "./": "./"
  },
  "keywords": [
    "tinyhttp",
    "node.js",
    "web framework",
    "web",
    "backend",
    "static",
    "cookie"
  ],
  "author": "v1rtl",
  "license": "MIT",
  "devDependencies": {
    "tsup": "^3.4.2",
    "typescript": "^3.9.7"
  },
  "scripts": {
    "build": "tsup src/index.ts --minify-whitespace --format cjs,esm --dts"
  }
}
